	pipeline {
	    agent any
             tools {
        		maven 'Maven 3.9.9' // Name must match the one in Jenkins Global Tool Configuration
    		   }
	    stages {
	        stage('Clean') {
	            steps {
	                sh 'mvn clean'
	            }
	        }
	        stage('Compile') {
	            steps {
	                sh 'mvn compile'
	            }
	        }
	        stage('test') {
	            steps {
	                sh 'mvn test'
	            }
	        }
	        stage('package') {
	            steps {
	                sh 'mvn package'
	            }
	        }
	        stage('Install') {
	            steps {
	                sh 'mvn install'
	            }
	        }
	    }
	}

